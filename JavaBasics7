package OOpsConcept;

public class FuntionInJava {

	public static void main(String[] args) {
		FuntionInJava obj= new FuntionInJava();
		
		obj.test();
		
		int l=obj.pqr();
		System.out.println(l);
		
		String s1=obj.ab();
		System.out.println(s1);
		
		int div=obj.division(30, 10);
		System.out.println(div);
	}
	
	
		//non static method
		public void test(){
			System.out.println("test method");
		}
		
		
		//Return type-->int
		public int pqr() {
			System.out.println("pqr method");
			int a=10;
			int b=20;
			int c=a+b;
			
			return c;
		}
		
		
		//Return type-->String	
		public String ab(){
			System.out.println("qa method");
			String s="Selenium";
			
			return s;
		}
		
		
		//Division Method
		public int division(int x, int y){
			System.out.println("division method");
			
			int d=x/y;
			
			return d;
		}
		
		

	}


